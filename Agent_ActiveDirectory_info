SELECT PREP.PCO_LOGIN.LOGIN, PREP.PCO_LOGIN.STATUS, PREP.PCO_LOGIN.ACTIVEDIRECTORYUSER, PREP.PCO_AGENT.NAME, PREP.PCO_AGENT.NICKNAME
FROM  PREP.PCO_LOGINAGENT INNER JOIN
         PREP.PCO_LOGIN ON PREP.PCO_LOGINAGENT.LOGIN = PREP.PCO_LOGIN.LOGIN INNER JOIN
         PREP.PCO_AGENT ON PREP.PCO_LOGINAGENT.AGENTID = PREP.PCO_AGENT.ID
		WHERE ACTIVEDIRECTORYUSER = ' ' or ACTIVEDIRECTORYUSER = 'minenhleshozi@CoActivate.co.za' or ACTIVEDIRECTORYUSER = 'debbie-annecoetzer@CoActivate.co.za'
		ORDER BY ACTIVEDIRECTORYUSER DESC
